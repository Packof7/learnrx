{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "intro-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/intro/IntroWebPart.js",
          "manifest": "./src/webparts/intro/IntroWebPart.manifest.json"
        }
      ]
    },
    "pack-client-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/packClient/PackClientWebPart.js",
          "manifest": "./src/webparts/packClient/PackClientWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "IntroWebPartStrings": "lib/webparts/intro/loc/{locale}.js",
    "PackClientWebPartStrings": "lib/webparts/packClient/loc/{locale}.js"
  }
}
